<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.backend.entity.TeacherStudentEntity">
        <table name="teacher_student" schema="self_education" catalog=""/>
        <attributes>
            <id name="idTeacherStudent">
                <column name="id_teacher_student"/>
            </id>
            <basic name="idTeacher">
                <column name="id_teacher"/>
            </basic>
            <basic name="idStudent">
                <column name="id_student"/>
            </basic>
            <many-to-one name="teacherByIdTeacher" optional="true" target-entity="com.backend.entity.TeacherEntity">
                <join-column name="id_teacher" referenced-column-name="id_teacher"/>
            </many-to-one>
            <many-to-one name="studentByIdStudent" optional="true" target-entity="com.backend.entity.StudentEntity">
                <join-column name="id_student" referenced-column-name="id_student"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
