<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.backend.entity.SubjectEntity">
        <table name="subject" schema="self_education" catalog=""/>
        <attributes>
            <id name="idSubject">
                <column name="id_subject"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <one-to-many name="productsByIdSubject" mapped-by="subjectByIdSubject"
                         target-entity="com.backend.entity.ProductEntity"/>
            <one-to-many name="teachersByIdSubject" mapped-by="subjectByIdSubject"
                         target-entity="com.backend.entity.TeacherEntity"/>
        </attributes>
    </entity>
</entity-mappings>
